{
    "name": "lazzard/ftp-bridge",
    "description": "A low-level implementation library of the File Transfer Protocol (FTP) in PHP that follows the RFC 959 standards and others related RFC extensions",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "ftp-protocol",
        "ftp",
        "low-level"
    ],
    "authors": [
        {
            "name": "El Amrani Chakir",
            "email": "elamrani.sv.laza@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mikey179/vfsstream": "^1.6",
        "dg/bypass-finals": "^1.3",
        "symfony/var-dumper": "^5.3",
        "filp/whoops": "^2.13"
    },
    "prefer-stable" : true,
    "autoload": {
        "psr-4": {
            "Lazzard\\FtpBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lazzard\\FtpBridge\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    }
}
